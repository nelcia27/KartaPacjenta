<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.chart.LineChart?>
<BorderPane fx:id="mainPane"
            styleClass="main_pane"
            stylesheets="/style/main.css"
            xmlns:fx="http://javafx.com/fxml/1"
            xmlns="http://javafx.com/javafx/10.0.2-internal">

    <top>
        <BorderPane>
            <left>
                <Button onAction="#returnTo" styleClass="filtrCombo">Wróć</Button>
            </left>
            <center>
                <VBox alignment="TOP_CENTER">
<!--                    Info-->
                </VBox>
            </center>
        </BorderPane>

    </top>
    <center>
        <VBox>
            <VBox fx:id="lineChartVBox"/>
            <HBox spacing="10" alignment="TOP_CENTER">
                <Button onAction="#prevClick" styleClass="filtrCombo" prefWidth="150">poprzedni</Button>
                <Button onAction="#plusClick" styleClass="filtrCombo" prefWidth="50">+</Button>
                <Button onAction="#minusClick" styleClass="filtrCombo" prefWidth="50">-</Button>
                <Button onAction="#nextClick" styleClass="filtrCombo" prefWidth="150">następny</Button>
            </HBox>
        </VBox>


    </center>

</BorderPane>
